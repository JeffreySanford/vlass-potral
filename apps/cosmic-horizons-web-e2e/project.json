{
  "name": "cosmic-horizons-web-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/cosmic-horizons-web-e2e/src",
  "implicitDependencies": ["cosmic-horizons-web"],
  "// targets": "to see all targets run: nx show project cosmic-horizons-web-e2e --web",
  "targets": {
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "playwright test",
        "cwd": "apps/cosmic-horizons-web-e2e"
      }
    },
    "e2e-perf": {
      "executor": "nx:run-commands",
      "options": {
        "command": "playwright test src/perf.spec.ts --project=chromium",
        "cwd": "apps/cosmic-horizons-web-e2e"
      }
    }
  }
}
