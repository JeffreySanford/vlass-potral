{
  "name": "@vlass-portal/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:web": "nx serve vlass-web",
    "start:api": "nx serve vlass-api",
    "start:ports:free": "node scripts/ensure-ports-free.mjs",
    "start:infra:check": "node scripts/ensure-docker-ready.mjs",
    "start:infra": "pnpm run start:infra:check && docker compose down --remove-orphans --volumes && docker compose build --pull && docker compose up -d --wait",
    "start:all": "pnpm run start:ports:free && pnpm run start:infra && pnpm run start:ports:free && nx run-many --target=serve --projects=vlass-web,vlass-api --parallel=2 --skip-nx-cache",
    "start": "pnpm start:web",
    "test": "nx run-many --target=test --all",
    "test:web": "nx test vlass-web",
    "test:api": "nx test vlass-api",
    "e2e:mvp": "nx run mvp-gates:e2e",
    "build": "nx run-many --target=build --all",
    "lint": "nx run-many --target=lint --all",
    "lighthouse:mobile": "pnpm dlx @lhci/cli@0.14.0 autorun --config=.github/lighthouse/lighthouserc.json",
    "lighthouse:summary": "node scripts/summarize-lighthouse.mjs",
    "security:audit": "pnpm audit --prod --audit-level=high",
    "docs:check": "node scripts/check-doc-consistency.mjs",
    "quality:ci": "nx run docs-policy:check && nx run-many --target=test --all && nx run mvp-gates:e2e"
  },
